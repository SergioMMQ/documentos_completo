<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
       <record id="documentos_documentos_list_view_inherit" model="ir.ui.view">
            <field name="name">documentos.documentos.list.view.inherit</field>
            <field name="model">documentos.documentos</field>
            <field name="inherit_id" ref="document.documentos_documentos_view_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="attributes">
                    <attribute name="js_class">documentos_list_view</attribute>
                </xpath>
            </field>
        </record>
    
        <record id="documentos_documentos_kanban_view_inherit" model="ir.ui.view">
            <field name="name">documentos.documentos.kanban.view.inherit</field>
            <field name="model">documentos.documentos</field>
            <field name="inherit_id" ref="document.documentos_documentos_view_kanban"/>
            <field name="arch" type="xml">
                <xpath expr="//kanban" position="attributes">
                    <attribute name="js_class">documentos_kanban_view</attribute>
                </xpath>
            </field>
        </record>

    </data>
    

</odoo>
